# Secret key - provides privileged access, never expose to client
SUPABASE_SERVICE_ROLE_KEY=
# Your OpenAI API key for AI responses
OPENAI_API_KEY=
# Bcrypt hash of the shared MVP password (generate with scripts/generate-password-hash.ts)
PASSWORD_GATE_HASH=